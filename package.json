{
  "name": "angular-testing-course",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.json",
    "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "test": "ng test",
    "test:no-watch": "ng test --watch=false --code-coverage",
    "test:without-source-map": "ng test --source-map=false",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:prod": "ng build --prod",
    "start:prod": "http-server ./dist -a localhost -p 4200",
    "build-and-start:prod": "run-s build:prod start:prod",
    "e2e": "start-server-and-test build-and-start:prod http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.1",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "9.0.1",
    "@angular/compiler": "9.0.1",
    "@angular/core": "9.0.1",
    "@angular/forms": "9.0.1",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "9.0.0",
    "@angular/platform-browser": "9.0.1",
    "@angular/platform-browser-dynamic": "9.0.1",
    "@angular/router": "9.0.1",
    "body-parser": "^1.18.3",
    "core-js": "^2.6.11",
    "cypress": "^3.8.3",
    "express": "^4.16.2",
    "http-server": "^0.12.3",
    "moment": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "6.5.4",
    "tslib": "^1.13.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "9.0.1",
    "@angular/language-service": "9.0.1",
    "@cypress/webpack-preprocessor": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.46",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^6.0.0",
    "start-server-and-test": "^1.11.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "3.7.5"
  }
}
